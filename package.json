{
  "name": "mmir-plugin-asr-nuance-xhr",
  "version": "1.0.0",
  "description": "Cordova plugin for the MMIR framework that allows Automatic Speech Recognition (ASR) via Nuance web services",
  "main": "./www/webasrNuanceImpl.js",
  "directories": {
    "lib": "./www"
  },
  "types": "index.d.ts",
  "scripts": {
    "build": "createplugincompat ./ && pluginexport ./"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab-cos.b.dfki.de/mmir/mmir-plugin-asr-nuance-xhr.git"
  },
  "keywords": [
    "asr",
    "speech",
    "recognition",
    "nuance",
    "xhr"
  ],
  "author": "russa",
  "license": "MIT",
  "dependencies": {
    "mmir-plugin-encoder-amr": "git+https://gitlab-cos.b.dfki.de/mmir/mmir-plugin-encoder-amr.git"
  },
  "peerDependencies": {
    "mmir-lib": "git+https://gitlab-cos.b.dfki.de/mmir/dev_mmir-lib.git#develop/webpack"
  },
  "cordova": {
    "id": "mmir-plugin-asr-nuance-xhr",
    "platforms": []
  },
  "engines": {
    "cordovaDependencies": {
      "1.0.0": {
        "git+https://gitlab-cos.b.dfki.de/mmir/mmir-plugin-encoder-amr.git": ">=1.0.0"
      }
    }
  },
  "mmir": {
    "compat": {
      "./www/webasrNuanceImpl.js": {"file": "./www/alt/webasrNuanceImplCompat.js", "type": "asr"}
    }
  },
  "devDependencies": {
    "mmir-plugin-exports": "git+https://gitlab-cos.b.dfki.de/mmir/mmir-plugin-exports.git"
  }
}
